{
  // === Extensions ===
  "auto_install_extensions": {
    "dockerfile": true,
    "git-firefly": true,
    "java": true,
    "kotlin": true,
    "make": true,
    "maple-theme": true,
    "material-icon-theme": true,
    "tombi": true,
    "vue": true
  },

  // === AI ===
  "disable_ai": false,
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "model": "claude-3-7-sonnet",
      "provider": "zed.dev"
    },
    "dock": "left",
    "inline_assistant_model": {
      "model": "gemini-2.5-flash",
      "provider": "zed.dev"
    }
  },
  "context_servers": {
    "mcp-server-github": {
      "source": "extension",
      "enabled": true,
      "settings": {}
    }
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },

  // === Core Editor ===
  "auto_signature_help": false,
  "base_keymap": "VSCode",
  "buffer_font_family": "Maple Mono",
  "buffer_font_features": {
    "calt": true
  },
  "buffer_font_size": 16.0,
  "cursor_shape": "bar",
  "current_line_highlight": "gutter",
  "format_on_save": "off",
  "indent_guides": { "enabled": true },
  "relative_line_numbers": false,
  "show_completion_documentation": true,
  "show_completions_on_input": true,
  "show_signature_help_after_edits": false,
  "tab_size": 2,
  "ui_font_family": "Maple Mono",
  "ui_font_size": 16.0,

  // === Theme ===
  "icon_theme": "Material Icon Theme",
  "theme": {
    "dark": "Maple Dark",
    "light": "Maple Light",
    "mode": "system"
  },

  // === UI Elements ===
  "helix_mode": true,
  "bottom_dock_layout": "full",
  "inlay_hints": {
    "edit_debounce_ms": 300,
    "enabled": true,
    "scroll_debounce_ms": 50,
    "show_background": false,
    "show_other_hints": true,
    "show_parameter_hints": true,
    "show_type_hints": true
  },
  "inline_code_actions": false,
  "minimap": {
    "show": "never",
    "current_line_highlight": "gutter"
  },
  "middle_click_paste": false,
  "preview_tabs": {
    "enable_preview_from_code_navigation": true,
    "enabled": false
  },
  "project_panel": {
    "auto_reveal_entries": false,
    "dock": "right",
    "hide_root": true,
    "sticky_scroll": true
  },
  "scrollbar": {
    "axes": {
      "horizontal": true,
      "vertical": null
    },
    "cursors": true,
    "git_diff": true,
    "search_results": true,
    "selected_symbol": true,
    "selected_text": true,
    "show": "auto"
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": true
  },
  "tabs": {
    "git_status": true
  },
  "title_bar": {
    "show_branch_icon": true,
    "show_sign_in": false,
    "show_user_picture": true
  },
  "toolbar": {
    "agent_review": true,
    "breadcrumbs": false,
    "code_actions": false,
    "quick_actions": false,
    "selections_menu": true
  },
  "use_system_path_prompts": true,
  "use_system_prompts": false,

  // === Terminal ===
  "load_direnv": "shell_hook",
  "terminal": {
    "env": {
      "GIT_EDITOR": "zed -w",
      "VISUAL": "zed -w"
    },
    "font_family": "Maple Mono"
  },

  // === Privacy ===
  "auto_update": true,
  "restore_on_startup": "none",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  // === CALLING ===
  "audio": {
    "experimental.auto_speaker_volume": true,
    "experimental.denoise": true,
    "experimental.rodio_audio": true
  },

  // === Languages ===
  "languages": {
    "Go": {
      "formatter": "language_server",
      "hard_tabs": true,
      "inlay_hints": {
        "enabled": true,
        "show_other_hints": true,
        "show_parameter_hints": true,
        "show_type_hints": true
      },
      "language_servers": ["gopls"],
      "preferred_line_length": 100,
      "tab_size": 2
    },
    "Java": {
      "formatter": [
        {
          "code_action": "source.organizeImports"
        },
        "language_server"
      ],
      "hard_tabs": false,
      "inlay_hints": {
        "enabled": true,
        "show_other_hints": true,
        "show_parameter_hints": true,
        "show_type_hints": true
      },
      "language_servers": ["jdtls"],
      "tab_size": 2
    },
    "JavaScript": {
      "formatter": [
        {
          "code_action": "source.addMissingImports"
        },
        {
          "code_action": "source.organizeImports"
        },
        {
          "code_action": "source.fixAll.eslint"
        },
        "language_server"
      ],
      "inlay_hints": {
        "enabled": true,
        "show_other_hints": true,
        "show_parameter_hints": true,
        "show_type_hints": true
      },
      "language_servers": ["vtsls", "!typescript-language-server", "..."],
      "preferred_line_length": 100,
      "tab_size": 2
    },
    "JSON": {
      "tab_size": 2
    },
    "JSONC": {
      "tab_size": 2
    },
    "Kotlin": {
      "language_servers": ["kotlin-lsp", "!kotlin-language-server", "..."],
      "preferred_line_length": 100,
      "tab_size": 2
    },
    "Markdown": {
      "preferred_line_length": 100,
      "soft_wrap": "preferred_line_length"
    },
    "Rust": {
      "formatter": [
        {
          "code_action": "source.organizeImports"
        },
        {
          "code_action": "source.fixAll"
        },
        "language_server"
      ],
      "hard_tabs": false,
      "inlay_hints": {
        "enabled": true,
        "show_other_hints": true,
        "show_parameter_hints": true,
        "show_type_hints": true
      },
      "language_servers": ["rust-analyzer"],
      "preferred_line_length": 100,
      "show_edit_predictions": true,
      "tab_size": 2
    },
    "TSX": {
      "formatter": [
        {
          "code_action": "source.addMissingImports"
        },
        {
          "code_action": "source.organizeImports"
        },
        {
          "code_action": "source.fixAll.eslint"
        },
        "language_server"
      ],
      "inlay_hints": {
        "enabled": true,
        "show_other_hints": true,
        "show_parameter_hints": true,
        "show_type_hints": true
      },
      "language_servers": [
        "tailwindcss-language-server",
        "vtsls",
        "!typescript-language-server",
        "..."
      ],
      "preferred_line_length": 100,
      "tab_size": 2
    },
    "TypeScript": {
      "formatter": [
        {
          "code_action": "source.addMissingImports"
        },
        {
          "code_action": "source.organizeImports"
        },
        {
          "code_action": "source.fixAll.eslint"
        },
        "language_server"
      ],
      "inlay_hints": {
        "enabled": true,
        "show_other_hints": true,
        "show_parameter_hints": true,
        "show_type_hints": true
      },
      "language_servers": [
        "tailwindcss-language-server",
        "vtsls",
        "!typescript-language-server",
        "..."
      ],
      "preferred_line_length": 100,
      "tab_size": 2
    },
    "YAML": {
      "tab_size": 2
    }
  },

  // === LSP ===
  "lsp": {
    "eslint": {
      "settings": {
        "codeActionOnSave": {
          "enable": true,
          "mode": "all"
        },
        "format": {
          "enable": true
        },
        "workingDirectory": {
          "mode": "auto"
        }
      }
    },
    "gopls": {
      "initialization_options": {
        "analyses": {
          "unusedparams": true,
          "unusedvariable": true,
          "useany": true
        },
        "completeUnimported": true,
        "experimentalPostfixCompletions": true,
        "hints": {
          "assignVariableTypes": true,
          "compositeLiteralFields": true,
          "compositeLiteralTypes": true,
          "constantValues": true,
          "functionTypeParameters": true,
          "parameterNames": true,
          "rangeVariableTypes": true
        },
        "matcher": "Fuzzy",
        "semanticTokens": true,
        "staticcheck": true,
        "usePlaceholders": true
      }
    },
    "jdtls": {
      "initialization_options": {
        "bundles": []
      },
      "settings": {
        "java": {
          "autobuild": {
            "enabled": true
          },
          "cleanup": {
            "actionsOnSave": []
          },
          "codeAction": {
            "sortMembers": {
              "avoidVolatileChanges": true
            }
          },
          "codeGeneration": {
            "generateComments": false,
            "hashCodeEquals": {
              "useInstanceof": true,
              "useJava7Objects": true
            },
            "insertionLocation": "afterCursor",
            "toString": {
              "codeStyle": "STRING_CONCATENATION",
              "limitElements": 0,
              "listArrayContents": true,
              "skipNullValues": false,
              "template": "${object.className} [${member.name()}=${member.value}, ${otherMembers}]"
            },
            "useBlocks": true
          },
          "compile": {
            "nullAnalysis": {
              "mode": "automatic",
              "nonnull": [],
              "nullable": []
            }
          },
          "completion": {
            "chain": {
              "enabled": true
            },
            "enabled": true,
            "favoriteStaticMembers": [
              "org.junit.Assert.*",
              "org.junit.Assume.*",
              "org.junit.jupiter.api.Assertions.*",
              "org.junit.jupiter.api.Assumptions.*",
              "org.junit.jupiter.api.DynamicContainer.*",
              "org.junit.jupiter.api.DynamicTest.*",
              "org.mockito.Mockito.*",
              "org.mockito.ArgumentMatchers.*",
              "org.mockito.Answers.*"
            ],
            "filteredTypes": [
              "java.awt.*",
              "com.sun.*",
              "sun.*",
              "jdk.*",
              "org.graalvm.*",
              "io.micrometer.shaded.*"
            ],
            "guessMethodArguments": true,
            "importOrder": ["java", "javax", "com", "org"],
            "lazyResolveTextEdit": {
              "enabled": true
            },
            "matchCase": "firstLetter",
            "postfix": {
              "enabled": true
            }
          },
          "configuration": {
            "updateBuildConfiguration": "automatic"
          },
          "contentProvider": {
            "preferred": null
          },
          "errors": {
            "incompleteClasspath": {
              "severity": "warning"
            }
          },
          "foldingRange": {
            "enabled": true
          },
          "format": {
            "comments": {
              "enabled": true
            },
            "enabled": true,
            "onType": {
              "enabled": true
            },
            "settings": {
              "profile": null,
              "url": null
            }
          },
          "implementationsCodeLens": {
            "enabled": true
          },
          "import": {
            "exclusions": [
              "**/node_modules/**",
              "**/.metadata/**",
              "**/archetype-resources/**",
              "**/META-INF/maven/**",
              "/**/test/**"
            ],
            "gradle": {
              "arguments": null,
              "enabled": true,
              "home": null,
              "java": {
                "home": null
              },
              "jvmArguments": null,
              "offline": {
                "enabled": false
              },
              "user": {
                "home": null
              },
              "version": null,
              "wrapper": {
                "enabled": true
              }
            },
            "maven": {
              "enabled": true,
              "offline": {
                "enabled": false
              }
            }
          },
          "inlayHints": {
            "parameterNames": {
              "enabled": "all",
              "exclusions": []
            }
          },
          "jdt": {
            "ls": {
              "lombokSupport": {
                "enabled": true
              },
              "protobufSupport": {
                "enabled": false
              }
            }
          },
          "maxConcurrentBuilds": 1,
          "references": {
            "includeAccessors": true,
            "includeDecompiledSources": true
          },
          "referencesCodeLens": {
            "enabled": true
          },
          "saveActions": {
            "organizeImports": true
          },
          "selectionRange": {
            "enabled": true
          },
          "semanticHighlighting": {
            "enabled": true
          },
          "signatureHelp": {
            "description": {
              "enabled": true
            },
            "enabled": true
          },
          "sources": {
            "organizeImports": {
              "starThreshold": 99,
              "staticStarThreshold": 99
            }
          },
          "templates": {
            "fileHeader": [],
            "typeComment": []
          },
          "trace": {
            "server": "off"
          },
          "typeHierarchy": {
            "lazyLoad": false
          }
        }
      }
    },
    "ruff": {
      "initialization_options": {
        "settings": {
          "exclude": ["*"]
        }
      }
    },
    "rust-analyzer": {
      "initialization_options": {
        "assist": {
          "emitMustUse": true,
          "expressionFillDefault": "default",
          "preferSelf": true,
          "termSearch": {
            "borrowcheck": true,
            "fuel": 2000
          }
        },
        "cargo": {
          "allFeatures": true,
          "allTargets": true,
          "autoreload": true,
          "buildScripts": {
            "enable": true,
            "rebuildOnSave": true,
            "useRustcWrapper": true
          },
          "features": "all",
          "loadOutDirsFromCheck": true
        },
        "check": {
          "allTargets": true,
          "command": "clippy",
          "extraArgs": [
            "--",
            "-W",
            "clippy::all",
            "-W",
            "clippy::pedantic",
            "-W",
            "clippy::nursery",
            "-W",
            "clippy::cargo",
            "-A",
            "clippy::module_name_repetitions",
            "-A",
            "clippy::missing_errors_doc",
            "-A",
            "clippy::missing_panics_doc"
          ],
          "workspace": false
        },
        "checkOnSave": false,
        "completion": {
          "addCallArgumentSnippets": true,
          "addCallParenthesis": true,
          "autoimport": {
            "enable": true
          },
          "callable": {
            "snippets": "fill_arguments"
          },
          "fullFunctionSignatures": {
            "enable": true
          },
          "postfix": {
            "enable": true
          },
          "privateEditable": {
            "enable": true
          },
          "termSearch": {
            "enable": true,
            "fuel": 2000
          }
        },
        "diagnostics": {
          "disabled": ["unresolved-proc-macro", "unresolved-method"],
          "enable": true,
          "enableExperimental": true,
          "styleLints": {
            "enable": true
          }
        },
        "highlightRelated": {
          "breakPoints": {
            "enable": true
          },
          "closureCaptures": {
            "enable": true
          },
          "exitPoints": {
            "enable": true
          },
          "references": {
            "enable": true
          },
          "yieldPoints": {
            "enable": true
          }
        },
        "hover": {
          "actions": {
            "debug": true,
            "enable": true,
            "gotoTypeDef": true,
            "implementations": true,
            "references": true,
            "run": true
          },
          "documentation": {
            "enable": true,
            "keywords": {
              "enable": true
            }
          },
          "links": {
            "enable": true
          },
          "memoryLayout": {
            "alignment": "hexadecimal",
            "enable": true,
            "offset": "hexadecimal",
            "size": "both"
          },
          "show": {
            "enumVariants": 10,
            "fields": 10,
            "traitAssocItems": 10
          }
        },
        "imports": {
          "granularity": {
            "enforce": false,
            "group": "crate"
          },
          "group": {
            "enable": true
          },
          "merge": {
            "glob": true
          },
          "prefix": "by_crate"
        },
        "inlayHints": {
          "bindingModeHints": {
            "enable": true
          },
          "chainingHints": {
            "enable": true
          },
          "closingBraceHints": {
            "enable": true,
            "minLines": 10
          },
          "closureCaptureHints": {
            "enable": true
          },
          "closureReturnTypeHints": {
            "enable": "with_block"
          },
          "closureStyle": "impl_fn",
          "discriminantHints": {
            "enable": "fieldless"
          },
          "expressionAdjustmentHints": {
            "enable": "never"
          },
          "genericParameterHints": {
            "const": {
              "enable": true
            },
            "lifetime": {
              "enable": true
            },
            "type": {
              "enable": true
            }
          },
          "implicitDrops": {
            "enable": false
          },
          "lifetimeElisionHints": {
            "enable": "skip_trivial",
            "useParameterNames": true
          },
          "maxLength": 30,
          "parameterHints": {
            "enable": true
          },
          "rangeExclusiveHints": {
            "enable": true
          },
          "renderColons": true,
          "typeHints": {
            "enable": true,
            "hideClosureInitialization": false,
            "hideNamedConstructor": false
          }
        },
        "joinLines": {
          "joinAssignments": true,
          "joinElseIf": true,
          "removeTrailingComma": true,
          "unwrapTrivialBlock": true
        },
        "lens": {
          "debug": true,
          "enable": true,
          "implementations": true,
          "location": "above_name",
          "references": {
            "adt": {
              "enable": true
            },
            "enumVariant": {
              "enable": true
            },
            "method": {
              "enable": true
            },
            "trait": {
              "enable": true
            }
          },
          "run": true
        },
        "lru": {
          "capacity": 256
        },
        "procMacro": {
          "attributes": {
            "enable": true
          },
          "enable": true,
          "ignored": {
            "leptos_macro": ["server"]
          }
        },
        "references": {
          "excludeImports": false,
          "excludeTests": false
        },
        "runnables": {
          "extraArgs": ["--release"],
          "extraTestBinaryArgs": ["--show-output", "--nocapture"]
        },
        "rustfmt": {
          "extraArgs": [],
          "overrideCommand": null,
          "rangeFormatting": {
            "enable": false
          }
        },
        "semanticHighlighting": {
          "nonStandardTokens": true,
          "operator": {
            "enable": true,
            "specialization": {
              "enable": true
            }
          },
          "punctuation": {
            "enable": true,
            "specialization": {
              "enable": true
            }
          },
          "strings": {
            "enable": true
          }
        },
        "signatureInfo": {
          "detail": "full",
          "documentation": {
            "enable": true
          }
        },
        "workspace": {
          "symbol": {
            "search": {
              "excludeImports": false,
              "kind": "all_symbols",
              "limit": 256,
              "scope": "workspace_and_dependencies"
            }
          }
        }
      }
    },
    "vtsls": {
      "settings": {
        "javascript": {
          "inlayHints": {
            "enumMemberValues": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": false
            }
          },
          "preferences": {
            "importModuleSpecifier": "relative",
            "quoteStyle": "single"
          },
          "suggest": {
            "autoImports": true,
            "completeFunctionCalls": true,
            "includeCompletionsForModuleExports": true,
            "includeCompletionsWithInsertText": true
          },
          "tsserver": {
            "maxTsServerMemory": 8092
          },
          "updateImportsOnFileMove": {
            "enabled": "always"
          }
        },
        "typescript": {
          "inlayHints": {
            "enumMemberValues": {
              "enabled": true
            },
            "functionLikeReturnTypes": {
              "enabled": true
            },
            "parameterNames": {
              "enabled": "all",
              "suppressWhenArgumentMatchesName": false
            },
            "parameterTypes": {
              "enabled": true
            },
            "propertyDeclarationTypes": {
              "enabled": true
            },
            "variableTypes": {
              "enabled": true,
              "suppressWhenTypeMatchesName": false
            }
          },
          "preferences": {
            "importModuleSpecifier": "relative",
            "quoteStyle": "single"
          },
          "suggest": {
            "autoImports": true,
            "completeFunctionCalls": true,
            "includeCompletionsForModuleExports": true,
            "includeCompletionsWithInsertText": true
          },
          "tsserver": {
            "maxTsServerMemory": 8092
          },
          "updateImportsOnFileMove": {
            "enabled": "always"
          }
        }
      }
    }
  }
}
