[init]
	defaultBranch = master

[core]
	editor = zeditor --wait
	autocrlf = input

[pull]
	rebase = true

[push]
	autoSetupRemote = true

[color]
	ui = true

[color "status"]
	added = green
	changed = yellow
	untracked = red

[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green

[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
